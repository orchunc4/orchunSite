{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "functions": {
        "api/index.js": {
            "maxDuration": 60
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/index.js"
        },
        {
            "source": "/((?!api).*)",
            "destination": "/index.html"
        }
    ]
}